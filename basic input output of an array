#include<stdio.h>
int main()
{
    int a[1000],b[1000],n,i;
    printf("enter the no of elements you want to enter in the array");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        printf("enter array element[%d]",i);
        scanf("%d",&a[i]);

    }
    for(i=0;i<n;i++)
    {
        printf("%d",a[i]);
    }
    return;
}
